lib:
	gcc -Wall -Werror ecores.c libs/xdraw.c libs/ximage.c libs/xmath.c libs/xpng.c libs/xy.c -o ecores_exe
	gcc -Wall -Werror etexto.c libs/xdraw.c libs/ximage.c libs/xmath.c libs/xpng.c libs/xy.c -o etexto_exe
	gcc -Wall -Werror edraw.c libs/xdraw.c libs/ximage.c libs/xmath.c libs/xpng.c libs/xy.c -o edraw_exe
	./ecores_exe
	./etexto_exe
	./edraw_exe
full:
	gcc -Wall -Werror ecores.c  -o ecores_exe
	gcc -Wall -Werror etexto.c  -o etexto_exe
	gcc -Wall -Werror edraw.c   -o edraw_exe
	./ecores_exe
	./etexto_exe
	./edraw_exe
